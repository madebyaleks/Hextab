{
  "name": "Hexclock new tab",
  "description": "Displays a customized version of Connor Montgomery's Hex clock in a new tab.",
  "version": "1",
  "manifest_version": 2,
  "chrome_url_overrides":
  {
    "newtab": "theclock.html"
  },
  "background" : {
    "scripts" : ["background.js"],
    "persistent" : true
  },
  "icons" : {
    "32": "icons/hextab-32px.png",
    "48": "icons/hextab-48px.png",
    "64": "icons/hextab-64px.png",
    "128": "icons/hextab-128px.png",
    "512": "icons/hextab.png"
  },
  "options_page": "settings.html",
  "web_accessible_resources": [
    "settings.html"
  ]
}