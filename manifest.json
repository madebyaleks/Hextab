{
  "author": "Made by Aleks",
  "name": "Hextab",
  "description": "Ever wondered what colour the time is? Now you can find out! This extension replaces your new tab page with a minimal hexclock.",
  "version": "1.2",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "icons": {
    "32": "depends/icons/hextab-32px.png",
    "48": "depends/icons/hextab-48px.png",
    "64": "depends/icons/hextab-64px.png",
    "128": "depends/icons/hextab-128px.png",
    "512": "depends/icons/hextab.png"
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    "options.html",
    //"hello.html"
  ]
}